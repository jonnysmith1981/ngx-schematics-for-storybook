{
  "name": "ngx-schematics-for-storybook",
  "version": "0.2.1",
  "description": "Angular schematics which automatically adds new components created with `ng generate component` to the [Storybook](https://storybook.js.org/)",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js"
  },
  "keywords": [
    "schematics",
    "angular",
    "storybook"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kimamula/ngx-schematics-for-storybook.git"
  },
  "author": "kimamula",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "devDependencies": {
    "@angular-devkit/core": "^0.8.3",
    "@angular-devkit/schematics": "^0.8.3",
    "@schematics/angular": "^0.8.3",
    "@types/babel__generator": "^7.0.0",
    "@types/babel__parser": "^7.0.0",
    "@types/babel__traverse": "^7.0.0",
    "@types/jasmine": "^2.6.0",
    "@types/node": "^10.11.0",
    "jasmine": "^3.2.0",
    "json5": "^2.0.1",
    "ts-node": "^7.0.1",
    "typescript": "2.9.2"
  },
  "dependencies": {
    "@babel/generator": "^7.0.0",
    "@babel/parser": "^7.1.0",
    "@babel/traverse": "^7.1.0"
  },
  "files": [
    "src/**/files",
    "src/**/*.js",
    "src/**/*.json",
    "src/**/*.d.ts",
    "!src/**/*_spec.js",
    "!src/component/createSchema.js"
  ]
}
